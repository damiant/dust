              "media:rating": {
                "$text": "nonadult",
                "scheme": "urn:simple"
              },
              "media:description": {
                "$text": "Dialog from MacOS Sequoia showing \"ESBuild\" not opened because Apple could not verify \"esbuild\" is free of malware that may harm your Mac or compromise your privacy.",
                "type": "plain"
